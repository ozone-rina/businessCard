	<select id="mySelectByExample" parameterType="com.example.demo.entity.PersonalinfotableExample" resultMap="BaseResultMap">
    SELECT p.*, c.company AS company
    FROM personalinfotable p
    LEFT JOIN companytable c ON p.companyId = c.companyId
    <where>
        <if test="name != null and name != ''">
            p.NAME LIKE concat('%', #{name}, '%') OR c.COMPANY LIKE concat('%', #{name}, '%')
        </if> OR p.DEPARTMENT LIKE concat('%', #{name}, '%')
        OR p.POST LIKE concat('%', #{name}, '%')
        OR p.ADDRESS LIKE concat('%', #{name}, '%')
        OR p.TEL LIKE concat('%', #{name}, '%')
        OR p.MAIL LIKE concat('%', #{name}, '%')
        OR p.PROJECTTITLE LIKE concat('%', #{name}, '%')
        OR p.REMARKS LIKE concat('%', #{name}, '%')
		</where>
	</select>
	
	
